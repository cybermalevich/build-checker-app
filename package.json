{
  "name": "build-checker-app",
  "version": "0.0.10",
  "main": "main.js",
  "productName": "BuildCheckerApp",
  "repository": "https://github.com/willmendesneto/build-checker-app",
  "description": "Electron application boilerplate based on React, React Router, Webpack, React Hot Loader for rapid application development",
  "scripts": {
    "build-css": "node-sass app/styles/main.scss app/app.global.css && node-sass app/styles/main.scss app/app.global.css --output-style compressed -w --recursive app/styles",
    "new-test": "NODE_PATH=$NODE_PATH:$PWD/src/js:$PWD/src/js/components mocha --compilers js:babel-core/register --require ignore-styles --require ./test/test_helper.js './src/js/**/*_spec.@(js|jsx)'",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --recursive --require ./test/setup.js test/**/*.spec.js",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --require ./test/setup.js --require co-mocha ./test/e2e.js",
    "pretest": "eslint app",
    "hot-server": "node server.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron ./",
    "package": "cross-env NODE_ENV=production node package.js",
    "package-all": "npm run package -- --all",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "dev": "concurrently --kill-others \"npm run build-css\" \"npm run hot-server\" \"npm run start-hot\""
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "author": {
    "name": "Will Mendes",
    "email": "willmendesneto@gmail.com",
    "url": "https://github.com/willmendesneto"
  },
  "license": "MIT",
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "react-router",
    "flux",
    "webpack",
    "react-hot"
  ],
  "homepage": "https://github.com/willmendesneto/build-checker-app#readme",
